{
	"$schema": "https://biomejs.dev/schemas/1.7.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"javascript":{
		"formatter":{
			"quoteStyle":"single",
			"trailingComma":"es5"
		}
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"correctness":{
				"noUnusedImports":"error"
			}
		}
	}
}
